---
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>hawksworx.com</title>
  <link href="https://hawksworx.com/"/>
  <link type="application/atom+xml" rel="self" href="https://hawksworx.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://hawksworx.com/</id>
  <author>
    <name>Phil Hawksworth</name>
    <email>phawksworth@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>https://hawksworx.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="https://hawksworx.com{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Phil Hawksworth</name>
      <uri>https://hawksworx.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
